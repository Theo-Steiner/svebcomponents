{
  "name": "@svebcomponents/ssr",
  "version": "0.0.1",
  "scripts": {
    "build": "svelte-package && publint",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./wrapper-component": {
      "svelte": "./dist/vite/CustomElementWrapper.svelte"
    },
    "./vite": {
      "types": "./dist/vite/vitePluginSvebcomponentsSsr.d.ts",
      "import": "./dist/vite/vitePluginSvebcomponentsSsr.js"
    },
    "./rollup": {
      "types": "./dist/rollup/svebcomponentsSsrConfig.d.ts",
      "import": "./dist/rollup/svebcomponentsSsrConfig.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@lit-labs/ssr": "^3.3.1",
    "@lit-labs/ssr-dom-shim": "1.3.0",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-typescript": "12.1.2",
    "@rollup/plugin-virtual": "3.0.2",
    "@sveltejs/package": "^2.0.0",
    "@types/node": "22.13.14",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "magic-string": "0.30.17",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "rollup": "4.32.0",
    "rollup-plugin-svelte": "7.2.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "utils": "workspace:*",
    "vite": "^6.0.0",
    "zimmerframe": "1.1.2"
  },
  "keywords": [
    "svelte"
  ],
  "dependencies": {
    "esm-env": "1.2.2"
  }
}
