{
  "name": "@svebcomponents/prettier-config",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "pnpm /^lint:.*/",
    "lint:eslint": "eslint .",
    "lint:format": "prettier --check .",
    "fix": "pnpm /^fix:.*/",
    "fix:lint": "eslint . --fix",
    "fix:format": "prettier --write ."
  },
  "exports": {
    "./base": "./src/base.js",
    "./svelte": "./src/svelte.js"
  },
  "devDependencies": {
    "@svebcomponents/eslint-config": "workspace:*",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3"
  }
}
